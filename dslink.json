{
	"name": "CMXDSLink",
	"version": "1.1.0",
	"description": "Link for CMX Notification service",
	"license": "MIT",
	"author": {
		"name": "Lukáš Hruška",
		"email": "lukyhruska96@gmail.com"
	},
	"main": "bin/CMXDSLink",
	"repository": {
		"type": "git",
		"url": "https://github.com/SimacTechnik/CMXDSLink"
	},
	"bugs": {
		"url": "https://github.com/SimacTechnik/CMXDSLink/issues"
	},
	"configs": {
		"name": {
			"type": "string",
			"default": "CMX"
		},
		"broker": {
			"type": "url"
		},
		"token": {
			"type": "string"
		},
		"nodes": {
			"type": "path",
			"default": "nodes.json"
		},
		"key": {
			"type": "path",
			"default": ".key"
		},
		"log": {
			"type": "enum",
			"default": "info"
		},
		"handler_class": {
			"type": "string",
			"default": "cz.simac.cmxdslink.CMXDSLink"
		}
	}
}
